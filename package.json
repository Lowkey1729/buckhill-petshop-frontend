{
  "name": "buckhill-frontend-petshop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "tailwind": "npx tailwindcss -i ./src/assets/main.css -o ./public/css/tw-styles.css --watch",
    "tailwind-prod": "npx tailwindcss -i ./src/assets/main.css -o ./public/css/tw-styles.css --minify",
    "lint": "npx eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@eslint/js": "^9.6.0",
    "@mdi/font": "^7.4.47",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.14.10",
    "axios": "^1.7.2",
    "pinia": "^2.1.7",
    "swiper": "^11.1.4",
    "vue": "^3.4.29",
    "vue-router": "^4.4.0",
    "vuetify": "^3.6.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.4.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.3",
    "vitest": "^2.0.1",
    "vue-tsc": "^2.0.21"
  }
}
